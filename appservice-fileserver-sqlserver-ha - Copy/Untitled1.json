{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    "adSubnetPrefix": {
      "type": "string",
      "defaultValue": "10.0.0.0/24",
      "metadata": {
        "description": "AD Subnet Prefix"
      }
    },
    "FirstDCIP": {
      "type": "string",
      "minLength": 1,
      "maxLength": 3,
      "metadata": {
        "description": "Provide the latest octate of the IP Address for the First Domain Controller, for example 10"
      },
      "defaultValue": "10"
    },
    "SecondDCIP": {
      "type": "string",
      "minLength": 1,
      "maxLength": 3,
      "metadata": {
        "description": "Provide the latest octate of the IP Address for the Second Domain Controller, for example 11"
      },
      "defaultValue": "11"
    }
},
   "variables": {
   "adSubnetString": 
   "adstaticIP1": "[concat(variables('adsubnetstring'),parameters('FirstDCIP'))]"
   },
"resources": [],
    "outputs": {
        "ADSubnetOutput": {
            "value": "[substring(parameters('adSubnetPrefix'),0,sub(length(parameters('adSubnetPrefix')), 1))]",
            "type": "string"
        },
        "adstaticIP1Output": {
            "value": "[toUpper(variables('adstaticIP1'))]",
            "type": "string"
                }
    }
}